{
  "expo": {
    "name": "xepi",
    "slug": "xepi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/xepi-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/xepi-splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "API_URL": "${API_URL}"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    }
  }
}
